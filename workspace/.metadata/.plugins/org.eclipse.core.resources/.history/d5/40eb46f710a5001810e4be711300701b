package basico;

import clases.Cliente;


public class Equals {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
	Cliente c1 = new Cliente(1, "Pedro Jimenez");
	Cliente c2 = new Cliente(1, "Pedro Jimenez");
	
	if(c1.equals(c2)){
	System.out.println("Son iguales");	
	
	
	}else{
		System.out.println("No son iguales");
	
	}
	
	}

}
